<a href="{{{url}}}">
    <div class="card">
        <div class="card-header">
            <img src="{{default_image}}" class="card-img-top" alt="{{title}}">
            <h5 class="card-title">
                {{#shortentext}} 130, {{{title}}} {{/shortentext}}
            </h5>
            <div class="rating">
                {{> block_edupublisher/package_rating_display }}
            </div>
        </div>
